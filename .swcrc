{
	"sourceMaps": "inline",
	"module": {
		"type": "es6",
		"strict": true,
		"strictMode": true,
		"noInterop": false
	},
	"jsc": {
		"externalHelpers": false,
		"target": "es2022",
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": false,
			"react": {
				"throwIfNamespace": false,
				"development": false,
				"useBuiltins": false,
				"pragma": "React.createElement",
				"pragmaFrag": "React.Fragment",
				"importSource": "react"
			}
		},
		"keepClassNames": true
	},
	"minify": false
}
